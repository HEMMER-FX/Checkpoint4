{% extends "base.html.twig" %}
    {% block body %}
        <div class="container-sm">
            {{ form_start(form) }}

            {{ form_label(form.name, 'Name', {'label_attr': {'class': 'form-label'}}) }}
            {{ form_widget(form.name) }}

            {{ form_rest(form) }}
                <input class="btn btn-outline-warning" type='submit' value="Envoyer">
            {{ form_end(form) }}
        </div>
    {% endblock body %}